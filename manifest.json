{
  "manifest_version": 3,
  "name": "HyruleGuessr Filters",
  "description": "Advanced filters for HyruleGuessr",
  "version": "1.2",
  "content_scripts": [
    {
      "js": ["new-game.js"],
      "matches": [
        "https://hyruleguessr.com/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["mask.png"],
      "matches": ["https://hyruleguessr.com/*"]
    }
  ]

}
